# Configuration des clés API et services externes pour Paw Paths
# Copiez ce fichier vers .env et remplissez avec vos vraies clés API

# ===== SUPABASE (Base de données et authentification) =====
REACT_APP_SUPABASE_URL=https://your-project.supabase.co
REACT_APP_SUPABASE_ANON_KEY=your-supabase-anon-key
SUPABASE_SERVICE_ROLE_KEY=your-supabase-service-role-key

# ===== STRIPE (Paiements) =====
REACT_APP_STRIPE_PUBLISHABLE_KEY=pk_test_your-stripe-publishable-key
STRIPE_SECRET_KEY=sk_test_your-stripe-secret-key
STRIPE_WEBHOOK_SECRET=whsec_your-webhook-secret

# ===== GOOGLE MAPS (Géolocalisation et cartes) =====
REACT_APP_GOOGLE_MAPS_API_KEY=your-google-maps-api-key

# ===== TWILIO (SMS et notifications) =====
TWILIO_ACCOUNT_SID=your-twilio-account-sid
TWILIO_AUTH_TOKEN=your-twilio-auth-token
TWILIO_PHONE_NUMBER=+33123456789

# ===== SENDGRID (Emails) =====
SENDGRID_API_KEY=SG.your-sendgrid-api-key
SENDGRID_FROM_EMAIL=noreply@pawpaths.com
SENDGRID_FROM_NAME=Paw Paths

# ===== CLOUDINARY (Stockage d'images) =====
REACT_APP_CLOUDINARY_CLOUD_NAME=your-cloudinary-cloud-name
CLOUDINARY_API_KEY=your-cloudinary-api-key
CLOUDINARY_API_SECRET=your-cloudinary-api-secret

# ===== FIREBASE (Notifications push) =====
REACT_APP_FIREBASE_API_KEY=your-firebase-api-key
REACT_APP_FIREBASE_AUTH_DOMAIN=your-project.firebaseapp.com
REACT_APP_FIREBASE_PROJECT_ID=your-project-id
REACT_APP_FIREBASE_STORAGE_BUCKET=your-project.appspot.com
REACT_APP_FIREBASE_MESSAGING_SENDER_ID=123456789
REACT_APP_FIREBASE_APP_ID=1:123456789:web:abcdef123456
FIREBASE_ADMIN_SDK_KEY=your-firebase-admin-sdk-key

# ===== OPENWEATHER (API météo) =====
REACT_APP_OPENWEATHER_API_KEY=your-openweather-api-key

# ===== AGOLIA (Recherche avancée) =====
REACT_APP_ALGOLIA_APP_ID=your-algolia-app-id
REACT_APP_ALGOLIA_SEARCH_KEY=your-algolia-search-key
ALGOLIA_ADMIN_KEY=your-algolia-admin-key

# ===== SENTRY (Monitoring d'erreurs) =====
REACT_APP_SENTRY_DSN=https://your-sentry-dsn@sentry.io/project-id

# ===== MIXPANEL (Analytics) =====
REACT_APP_MIXPANEL_TOKEN=your-mixpanel-token

# ===== INTERCOM (Support client) =====
REACT_APP_INTERCOM_APP_ID=your-intercom-app-id

# ===== ZOOM (Vidéo conférence) =====
ZOOM_API_KEY=your-zoom-api-key
ZOOM_API_SECRET=your-zoom-api-secret

# ===== HUBSPOT (CRM) =====
HUBSPOT_API_KEY=your-hubspot-api-key
HUBSPOT_PORTAL_ID=your-hubspot-portal-id

# ===== MAILCHIMP (Marketing email) =====
MAILCHIMP_API_KEY=your-mailchimp-api-key
MAILCHIMP_LIST_ID=your-mailchimp-list-id

# ===== FACEBOOK/META (Authentification sociale) =====
REACT_APP_FACEBOOK_APP_ID=your-facebook-app-id
FACEBOOK_APP_SECRET=your-facebook-app-secret

# ===== GOOGLE (Authentification sociale) =====
REACT_APP_GOOGLE_CLIENT_ID=your-google-client-id
GOOGLE_CLIENT_SECRET=your-google-client-secret

# ===== APPLE (Authentification sociale) =====
APPLE_CLIENT_ID=your-apple-client-id
APPLE_TEAM_ID=your-apple-team-id
APPLE_KEY_ID=your-apple-key-id
APPLE_PRIVATE_KEY=your-apple-private-key

# ===== PAYPAL (Paiements alternatifs) =====
REACT_APP_PAYPAL_CLIENT_ID=your-paypal-client-id
PAYPAL_CLIENT_SECRET=your-paypal-client-secret

# ===== APPLE PAY / GOOGLE PAY =====
APPLE_PAY_MERCHANT_ID=merchant.com.pawpaths
GOOGLE_PAY_MERCHANT_ID=your-google-pay-merchant-id

# ===== RECAPTCHA (Protection anti-spam) =====
REACT_APP_RECAPTCHA_SITE_KEY=your-recaptcha-site-key
RECAPTCHA_SECRET_KEY=your-recaptcha-secret-key

# ===== AWS S3 (Stockage de fichiers) =====
AWS_ACCESS_KEY_ID=your-aws-access-key
AWS_SECRET_ACCESS_KEY=your-aws-secret-key
AWS_REGION=eu-west-3
AWS_S3_BUCKET=pawpaths-uploads

# ===== REDIS (Cache) =====
REDIS_URL=redis://localhost:6379
REDIS_PASSWORD=your-redis-password

# ===== ELASTICSEARCH (Recherche avancée) =====
ELASTICSEARCH_URL=https://your-elasticsearch-url
ELASTICSEARCH_USERNAME=your-elasticsearch-username
ELASTICSEARCH_PASSWORD=your-elasticsearch-password

# ===== CONFIGURATION GÉNÉRALE =====
NODE_ENV=production
REACT_APP_API_BASE_URL=https://api.pawpaths.com
REACT_APP_APP_URL=https://pawpaths.com
REACT_APP_SUPPORT_EMAIL=support@pawpaths.com
REACT_APP_SUPPORT_PHONE=+33123456789

# ===== SÉCURITÉ =====
JWT_SECRET=your-super-secret-jwt-key-min-32-chars
ENCRYPTION_KEY=your-encryption-key-32-chars
SESSION_SECRET=your-session-secret-key

# ===== RATE LIMITING =====
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=100

# ===== WEBHOOKS =====
WEBHOOK_SECRET=your-webhook-secret-key

# ===== MONITORING =====
HEALTH_CHECK_TOKEN=your-health-check-token
ADMIN_API_KEY=your-admin-api-key

# ===== DÉVELOPPEMENT UNIQUEMENT =====
# Ces variables ne doivent être utilisées qu'en développement
DEBUG=false
REACT_APP_DEBUG_MODE=false
ENABLE_MOCK_PAYMENTS=false
ENABLE_TEST_USERS=false
